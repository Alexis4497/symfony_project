{% block body %}
<link href="{{ asset('css/style_base_menu.css') }}" type="text/css" rel="stylesheet">
<link href="{{ asset('css/about.css') }}" type="text/css" rel="stylesheet">

 <div class="panel">
    <div class="panel-default"></div>
   
  </div>
  <nav class="black">
    <div class="underline"></div>
    <div class="underline"></div>
    <div class="underline"></div>
    <a href="{{ path('home')}}" onClick="ul(0)" >Accueil</a><a href="{{ path('about')}}" onClick="ul(1)">A Propos</a><a href="{{ path('skills')}}" onClick="ul(2)">Compétences</a><a href="{{ path('projects')}}" onClick="ul(3)">Projets</a><a href="{{ path('cv')}}" onClick="ul(4)">CV</a><a href="{{ path('contact')}}" onClick="ul(5)">Contact</a>
